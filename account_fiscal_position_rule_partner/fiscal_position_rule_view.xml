<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="view_account_fiscal_position_rule_form">
      <field name="name">account.fiscal.position.rule.form</field>
      <field name="model">account.fiscal.position.rule</field>
      <field name="inherit_id" ref="account_fiscal_position_rule.view_account_fiscal_position_rule_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='use_picking']" position="after">
          <field name="use_partner" />
        </xpath>
        <xpath expr="//separator[@string='Configuration']" position="before">
          <separator string='Partner' colspan="4"/>
          <field name='partner_country' />
          <field name='partner_state' />
        </xpath>
      </field>
    </record>
  </data>
</openerp>

